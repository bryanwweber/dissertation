month = oct,
number = {39},
pages = {9366--73},
pmid = {18702479},
title = {{Synchrotron photoionization mass spectrometry measurements of kinetics and product formation in the allyl radical (H2CCHCH2) self-reaction.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18702479},
volume = {112},
year = {2008}
}
