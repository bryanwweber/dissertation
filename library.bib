keywords = {Algorithms,Computer Simulation,Light,Models, Chemical,Models, Molecular,Nitrogen,Nitrogen: chemistry,Nitrogen: radiation effects,Photochemistry,Photochemistry: methods,Photons,Radiation, Ionizing,Spectrum Analysis,Spectrum Analysis: methods},
month = jul,
number = {25},
pages = {2958--63},
pmid = {16880908},
title = {{Imaging CIN(3) photodissociation from 234 to 280 nm.}},
