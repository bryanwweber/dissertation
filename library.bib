@article{Katsikadakos2012,
abstract = {Hydrogen abstraction reactions by the methyl radical from n-butanol have been investigated at the ROCBS-QB3 level of theory. Reaction energies and product geometries for the most stable conformer of n-butanol (ROH) have been computed, the reaction energies order $\alpha$ < $\gamma$ < $\beta$ < $\delta$ < OH. The preference for n-butane to favour H-abstraction at C($\beta$) and C($\gamma$) while, in contrast, n-butanol favours radical reactions at the C($\alpha$) carbon is rationalised. Transition state (TS) barriers and geometries for the most stable conformer of n-butanol are presented, and discussed with respect to the Hammond postulate. The reaction barriers order as $\alpha$ < OH < $\gamma$ < $\beta$ < $\delta$. This relative ordering is not consistent with product radical stability, C-H bond dissociation energies or previous studies using O[combining dot above]H and HO[combining dot above](2) radicals. We provide a molecular orbital based rationalisation for this ordering and answer two related questions: Why is the $\gamma$-channel more stable than the $\beta$-channel? Why do the two C($\gamma$)-H H-abstraction TS differ in energy? The method and basis set dependence of the TS barriers is investigated. The Boltzmann probability distribution for the n-butanol conformers suggests that low energy conformers are present in approximately equal proportions to the most stable conformer at combustion temperatures where ĊH(3) radicals are present. Thus, the relative significance of the various H-abstraction channels has been assessed for a selection of higher energy conformers (ROH'). Key results include finding that higher energy n-butanol conformers (E(ROH') > E(ROH)) can generate lower energy product radicals, E(ROH') < E(ROH). Moreover, higher energy conformers can also have a globally competitive TS energy for H-abstraction.},
author = {Katsikadakos, D. and Hardalupas, Y. and Taylor, A.M.K.P. and Hunt, P.a.},
doi = {10.1039/c2cp24074b},
file = {:Y$\backslash$:/Mendeley PDFs/Katsikadakos et al. - 2012 - Hydrogen abstraction from n-butanol by the methyl radical high level ab initio study of abstraction pathway.pdf:pdf},
issn = {1463-9084},
journal = {Phys. Chem. Chem. Phys.},
keywords = {1-Butanol,1-Butanol: chemistry,Hydrogen,Hydrogen: chemistry,Methane,Methane: analogs \& derivatives,Methane: chemistry,Molecular Conformation,Temperature,Thermodynamics},
month = jul,
number = {27},
pages = {9615--9629},
pmid = {22692370},
title = {{Hydrogen abstraction from n-butanol by the methyl radical: high level ab initio study of abstraction pathways and the importance of low energy rotational conformers.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22692370},
volume = {14},
year = {2012}
}
