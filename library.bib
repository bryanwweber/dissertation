keywords = {Biotechnology,Biotechnology: instrumentation,Biotechnology: methods,Butanols,Butanols: metabolism,Butyric Acid,Butyric Acid: metabolism,Clostridium,Clostridium: cytology,Clostridium: growth \& development,Clostridium: metabolism,Electrons,Glucose,Glucose: metabolism,Kinetics,Metabolic Networks and Pathways,Microbial Viability,Paraquat,Paraquat: metabolism,Recycling,Time Factors},
month = feb,
