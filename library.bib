journal = {Appl. Environ. Microbiol.},
keywords = {1-Butanol,1-Butanol: metabolism,1-Propanol,1-Propanol: metabolism,Acyltransferases,Acyltransferases: genetics,Acyltransferases: metabolism,Amino Acid Substitution,Biosynthetic Pathways,Biosynthetic Pathways: genetics,Butyric Acids,Butyric Acids: metabolism,Enzyme Inhibitors,Enzyme Inhibitors: pharmacology,Escherichia coli,Escherichia coli: genetics,Escherichia coli: metabolism,Folder - Biofuels,Glucose,Glucose: metabolism,Isoleucine,Isoleucine: pharmacology,Methanococcus,Methanococcus: enzymology,Missense,Mutagenesis,Mutation,Polymerase Chain Reaction,Pyruvic Acid,Pyruvic Acid: metabolism,Temperature},
