issn = {1520-5215},
journal = {J. Phys. Chem. A},
month = may,
number = {21},
pages = {5100--5111},
pmid = {22591104},
title = {{Detailed product analysis during low- and intermediate-temperature oxidation of ethylcyclohexane.}},
