@article{Kazakov2006,
abstract = {Computational singular perturbation (CSP) analysis has been used to gain understanding of the complex kinetic behavior associated with two-stage ignition of large hydrocarbon molecules. To this end, available detailed and reduced chemical kinetics models commonly used in numerical simulations of n-heptane oxidation phenomena are directly analyzed to interpret the underlying fundamental steps leading to two-stage ignition. Unlike previous implementations of the CSP methodology, temperature is included as one of the state variables so that factors controlling ignition can be unambiguously determined. The analyzed models show differences in the factors contributing to the initial development and shutdown of the first ignition stage. However, during the second stage, both models show the importance of the degenerate branching decomposition of hydrogen peroxide, which contradicts some previous interpretations of this phenomenon.},
author = {Kazakov, Andrei and Chaos, Marcos and Zhao, Zhenwei and Dryer, Frederick L.},
