doi = {10.1021/jp308460y},
file = {:Y$\backslash$:/Mendeley PDFs/Alecu et al. - 2012 - Biofuel combustion. Energetics and kinetics of hydrogen abstraction from carbon-1 in n-butanol by the hydroperoxyl.pdf:pdf},
