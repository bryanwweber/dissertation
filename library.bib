@article{Andrews-Hanna2012,
abstract = {The earliest history of the Moon is poorly preserved in the surface geologic record because of the high flux of impactors, but aspects of that history may be preserved in subsurface structures. Application of gravity gradiometry to observations by the Gravity Recovery and Interior Laboratory (GRAIL) mission results in the identification of a population of linear gravity anomalies with lengths of hundreds of kilometers. Inversion of the gravity anomalies indicates elongated positive density anomalies interpreted to be ancient vertical tabular intrusions or dikes, formed by magmatism in combination with extension of the lithosphere. Crosscutting relationships support a pre-Nectarian to Nectarian age, preceding the end of the heavy bombardment of the Moon. The distribution, orientation, and dimensions of the intrusions indicate a globally isotropic extensional stress state arising from an increase in the Moon's radius by 0.6 to 4.9 km early in lunar history, consistent with predictions of thermal models.},
author = {Andrews-Hanna, Jeffrey C and Asmar, Sami W and Head, James W and Kiefer, Walter S and Konopliv, Alexander S and Lemoine, Frank G and Matsuyama, Isamu and Mazarico, Erwan M. and McGovern, Patrick J and Melosh, H Jay and Neumann, Gregory A. and Nimmo, Francis and Phillips, Roger J and Smith, David E. and Solomon, Sean C and Taylor, G Jeffrey and Wieczorek, Mark a and Williams, James G and Zuber, Maria T},
doi = {10.1126/science.1231753},
file = {:Y$\backslash$:/Mendeley PDFs/Andrews-Hanna et al. - 2012 - Ancient Igneous Intrusions and Early Expansion of the Moon Revealed by GRAIL Gravity Gradiometry.pdf:pdf},
issn = {1095-9203},
journal = {Science},
month = dec,
number = {December},
pmid = {23223393},
title = {{Ancient Igneous Intrusions and Early Expansion of the Moon Revealed by GRAIL Gravity Gradiometry.}},
