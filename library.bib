issn = {1521-3773},
journal = {Angew. Chemie},
keywords = {Biofuels,Ethanol,Ethanol: chemistry,Folder - Biofuels,Oxygen,Oxygen: chemistry},
mendeley-tags = {Folder - Biofuels},
month = may,
number = {21},
pages = {3572--97},
pmid = {20446278},
title = {{Biofuel combustion chemistry: from ethanol to biodiesel.}},
url = {http://doi.wiley.com/10.1002/anie.200905335 http://www.ncbi.nlm.nih.gov/pubmed/20446278},
volume = {49},
year = {2010}
}
