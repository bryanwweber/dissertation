journal = {Appl. Microbiol. Biotechnol.},
keywords = {Bacteria,Bacteria: metabolism,Biofuels,Biofuels: analysis,Bioreactors,Bioreactors: microbiology,Ethanol,Ethanol: metabolism,Fungi,Fungi: metabolism,Industrial Microbiology,Industrial Microbiology: methods,Industrial Microbiology: trends,Lignin,Lignin: metabolism},
month = jul,
number = {4},
pages = {1303--15},
pmid = {20535464},
