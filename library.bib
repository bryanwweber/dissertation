journal = {J. Ind. Microbiol. Biotechnol.},
keywords = {Bioelectric Energy Sources,Butanols,Butanols: metabolism,Clostridium acetobutylicum,Clostridium acetobutylicum: genetics,Clostridium acetobutylicum: metabolism,Clostridium beijerinckii,Clostridium beijerinckii: genetics,Clostridium beijerinckii: metabolism,Escherichia coli,Escherichia coli: genetics,Escherichia coli: metabolism,Fermentation,Genetic Engineering,Genetic Engineering: methods,Industrial Microbiology,Industrial Microbiology: methods,Solvents,Solvents: metabolism},
month = sep,
