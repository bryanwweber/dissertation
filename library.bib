journal = {J. Phys. Chem. A},
keywords = {Acetates,Acetates: chemistry,Chemical,Esters,Esters: chemistry,Fires,Formic Acid Esters,Formic Acid Esters: chemistry,Isomerism,Models},
month = may,
number = {19},
pages = {4093--101},
pmid = {17388390},
title = {{Isomer-specific fuel destruction pathways in rich flames of methyl acetate and ethyl formate and consequences for the combustion chemistry of esters.}},
