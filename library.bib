@article{Calcagno2012,
abstract = {The study of science-making is a growing discipline that builds largely on online publication and citation databases, while prepublication processes remain hidden. Here, we report results from a large-scale survey of the submission process, covering 923 scientific journals from the biological sciences in years 2006-2008. Manuscript flows among journals revealed a modular submission network, with high-impact journals preferentially attracting submissions. However, about 75\% of published articles were submitted first to the journal that would publish them, and high-impact journals published proportionally more articles that had been resubmitted from another journal. Submission history affected postpublication impact: Resubmissions from other journals received significantly more citations than first-intent submissions, and resubmissions between different journal communities received significantly fewer citations.},
author = {Calcagno, V and Demoinet, E and Gollner, K and Guidi, L and Ruths, D and de Mazancourt, C},
doi = {10.1126/science.1227833},
file = {:Y$\backslash$:/Mendeley PDFs/Calcagno et al. - 2012 - Flows of Research Manuscripts Among Scientific Journals Reveal Hidden Submission Patterns.pdf:pdf},
