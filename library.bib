number = {50},
pages = {12206--12213},
pmid = {23151032},
title = {{Biofuel combustion. Energetics and kinetics of hydrogen abstraction from carbon-1 in n-butanol by the hydroperoxyl radical calculated by coupled cluster and density functional theories and multistructural variational transition-state theory with multidime}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23151032},
